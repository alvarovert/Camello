<link rel="stylesheet" href="~/css/Inicio.css" asp-append-version="true" />




<div class="wrapped">
    <div class="loginContainer">
        <img id="logoSurvey" src="https://surveyrental.com.pe/wp-content/uploads/2022/04/logo-survey-curvas.png" />
        <form asp-controller="Acceso" asp-action="Login" class="loginForm">
            <span class="inputIcon"></span>
            <input type="text" id="NDocumento" name="NDocumento" class="formInput" placeholder="Identificador" />
            <span class="inputIcon"></span>
            <input type="password" id="Password" name="Password" class="formInput" placeholder="Contraseña" />
            <button type="submit" id="butonLogin">Ingresar</button>
        </form>


    </div>




</div>


